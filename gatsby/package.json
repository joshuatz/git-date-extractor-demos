{
	"name": "git-date-extractor-gatsby-demo",
	"version": "1.0.0",
	"private": true,
	"description": "Git-Date-Extractor Gatsby Demo",
	"author": {
		"name": "Joshua Tzucker",
		"url": "https://joshuatz.com/?utm_source=package"
	},
	"keywords": [
		"gatsby"
	],
	"scripts": {
		"develop": "gatsby develop",
		"start": "gatsby develop",
		"build": "gatsby build",
		"serve": "gatsby serve",
		"clean": "gatsby clean",
		"dates": "git-date-extractor --outputToFile --onlyIn ./src/posts/"
	},
	"husky": {
		"hooks": {
			"pre-commit": "git diff --cached --name-only --diff-filter=ACMRTUXB -z | xargs -0 git-date-extractor --gitCommitHook=pre --outputToFile=true --projectRootPath='..' --onlyIn=['gatsby/src/posts'] --debug"
		}
	},
	"dependencies": {
		"flexboxgrid": "^6.3.1",
		"gatsby": "^2.30.2",
		"gatsby-plugin-react-helmet": "^3.8.0",
		"gatsby-plugin-sitemap": "^2.10.0",
		"gatsby-source-filesystem": "^2.9.0",
		"gatsby-transformer-remark": "^2.14.0",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-helmet": "^6.1.0"
	},
	"devDependencies": {
		"git-date-extractor": "^4.0.1",
		"husky": "^4.3.7"
	}
}
